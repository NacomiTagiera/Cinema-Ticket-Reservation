{
  "name": "cinema",
  "version": "1.0.0",
  "description": "Cinema Ticket Reservation System",
  "author": "Jakub Pawlak",
  "license": "ISC",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "tsc",
    "build:watch": "tsc -w",
    "start:server": "node dist/server/index.js",
    "start:client": "node dist/client/index.js",
    "dev:server": "ts-node src/server/index.ts",
    "dev:client": "ts-node src/client/index.ts",
    "lint": "eslint .",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "prettier": "prettier \"{src}/**/*.{ts,mts}\" --config .prettierrc --write",
    "prettier:check": "prettier \"{src}/**/*.{ts,mts}\" --config .prettierrc --check"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "express": "^4.21.1",
    "express-validator": "^7.2.0",
    "inquirer": "^12.0.1",
    "jsonwebtoken": "^9.0.2",
    "tslib": "~2.7"
  },
  "devDependencies": {
    "@prisma/client": "^5.21.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.7",
    "@eslint/js": "~9.10",
    "@types/eslint__js": "~8.42",
    "@types/node": "~20",
    "@typescript-eslint/parser": "~8.5",
    "eslint-config-prettier": "~9.1",
    "eslint": "~9.10",
    "globals": "~15.9",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "~3.3",
    "prisma": "^5.21.1",
    "rimraf": "~6.0",
    "ts-api-utils": "~1.3",
    "ts-node": "^10.9.2",
    "typescript-eslint": "~8.5",
    "typescript": "~5.5"
  },
  "volta": {
    "node": "20.17.0"
  }
}